{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Seq"
    ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "_Args": "this path is a magic string for logging to azure",
        "Args": { "path": "D:\\home\\LogFiles\\Application\\Program.txt" }
      },
      {
        "Name": "Seq",
        "Args": {
          "compact": true,
          "serverUrl": "http://iuslogging.westus.cloudapp.azure.com",
          "_apiKey": "todo: find a way to move this back to launchSettings.json",
          "apiKey": "olMmMaB7qErIxYq65kNR"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": { "maximumDestructuringDepth": 3 }
      }
    ]
  },
  "AppSettings": {
    "BaseAddress": "",
    "Projects": [],
    "Security": {
      "Username": "",
      "Password": ""
    },
    "Authorization": {
      "AppId": "",
      "AppSecret": "",
      "State": "User",
      "RedirectURI": "/data/Login/LoginAuth",
      "ClientId": "",
      "ClientSecret": "",
      "TenantId": ""
    }
  }
}